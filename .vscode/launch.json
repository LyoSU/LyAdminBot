{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Bot (index.js)",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/index.js",
      "runtimeExecutable": "/Users/ly/.nvm/versions/node/v22.2.0/bin/node",
      "env": {
        "NODE_ENV": "development"
      },
      "console": "integratedTerminal",
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "name": "Launch Bot (bot.js)",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/bot.js",
      "runtimeExecutable": "/Users/ly/.nvm/versions/node/v22.2.0/bin/node",
      "env": {
        "NODE_ENV": "development"
      },
      "console": "integratedTerminal",
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "name": "Launch Bot (NVM Auto)",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/index.js",
      "runtimeExecutable": "${env:HOME}/.nvm/nvm-exec",
      "runtimeArgs": ["node"],
      "env": {
        "NODE_ENV": "development"
      },
      "console": "integratedTerminal",
      "skipFiles": [
        "<node_internals>/**"
      ]
    },
    {
      "name": "Attach to Process",
      "type": "node",
      "request": "attach",
      "port": 9229,
      "skipFiles": [
        "<node_internals>/**"
      ]
    }
  ]
}
